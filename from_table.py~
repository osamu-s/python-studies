#!/usr/bin/env python

import json


def tokenize(tbl_str, frame=0, sep="+"):
    print (tbl_str)
    sep_idxs = [ i for i, c in enumerate(tbl_str[frame])
                 if c == sep ]
    slices = [ (s+1, e-1) for s, e in zip([0]+sep_idxs, sep_idxs)[1:]]
    
    print (slices)
    return [[ln[s:e].strip() for s, e in slices]
            for ln in tbl_str if not ln.startswith(sep)]

input_table = """+-------+-------+-------+
| item1 | item2 | item3 |
+-------+-------+-------+
| a1    | a2    | a3    |
| b1    | b2    | b3    |
+-------+-------+-------+
"""

def main():
    print (tokenize(input_table.splitlines()))
    

if __name__ == '__main__':
        main()
